{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "js": "./src",
                    "jsx": "./src"
                }
            }
        ],
        "@babel/plugin-transform-runtime",
        [
            "styled-components",
            {
                "ssr": true,
                "preprocess": false
            }
        ]
    ]
}